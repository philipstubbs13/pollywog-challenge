{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-filename-extension": 0,
      "react/prefer-stateless-function": 0,
      "linebreak-style": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-one-expression-per-line": 0,
      "no-underscore-dangle": 0,
      "class-methods-use-this": 0,
      "no-prototype-builtins": 0,
      "no-param-reassign": 0
    }
}